{
  "manifest_version": 3,
  "name": "Click Here!",
  "version": "1.0.0",

  "background": {
    "service_worker": "background.js",
    "type": "module"
  },

  "action": {
    "default_popup": "popup/popup.html"
  },

  "permissions": [
    "scripting"
  ],

  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
